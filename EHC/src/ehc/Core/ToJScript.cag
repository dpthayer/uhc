  | Case        loc         .   scrutineeCVarInfo =   CVarInfo_None

%%[(8 jscript)
SEM CExpr
  | Case        loc         .   (scrutineeTagJS,scrutineeCVarInfo)
                                                =   case @alts.scrutinees of
                                                      (Scrutinee_Var nm : _)
                                                        -> panic ("Core.ToJScript.CExpr.Case.Scrutinee_Var: " ++ show nm ++ " : not yet implemented")
                                                      (Scrutinee_Int _ : _)
                                                        -> ( @expr.js
                                                           , CVarInfo_None
