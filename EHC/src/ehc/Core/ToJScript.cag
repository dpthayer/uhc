  | App         lhs         .   resPackWrap     =   if @isTopApp' then id else @func.resPackWrap
  | * - FFI App Ann
                lhs         .   resPackWrap     =   id
%%]

%%[(8 jscript)
ATTR CExpr [ | | mkFFI : {J.Expr -> [J.Expr] -> J.Expr} ]

SEM CExpr
