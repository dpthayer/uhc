ATTR CExpr [ | | argUnpackWrapL : {[J.Expr -> J.Expr]} ]

SEM CExpr
  | FFI         lhs         .   argUnpackWrapL  =   @mkArgsJS
  | App         (loc.argUnpackWrap,loc.argUnpackWrapL)
                                                =   hdAndTl' id @func.argUnpackWrapL
                lhs         .   argUnpackWrapL  =   if @isTopApp' then [] else @argUnpackWrapL
  | * - FFI App Ann
                lhs         .   argUnpackWrapL  =   []
