%%[(8 codegen)
SEM MbExpr
  | Nothing     lhs         .   ppMb        =   Nothing
  | Just        lhs         .   ppMb        =   Just @just.pp

SEM MbMetas
  | Nothing     lhs         .   ppMb        =   Nothing
  | Just        lhs         .   ppMb        =   Just @just.pp

SEM MbMetaVal
  | Nothing     lhs         .   ppMb        =   Nothing
  | Just        lhs         .   ppMb        =   Just @just.pp
%%]

