%%[(8 codegen)
SEM Expr
  | Arrow Lam   loc         .   lamBodyPP   =   @res.lamBodyPP
                            .   lamArgPPL   =   @arg.pp : @res.lamArgPPL
  | * - Lam Arrow
                lhs         .   lamBodyPP   =   @pp
                            .   lamArgPPL   =   []
%%]

