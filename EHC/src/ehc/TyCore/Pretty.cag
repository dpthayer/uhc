  | App         loc         .   appFunPP    =   @func.appFunPP
                            .   appArgPPL   =   @arg.pp : @func.appArgPPL
  | * - App     lhs         .   appFunPP    =   @pp
                            .   appArgPPL   =   []
%%]

%%[(8888 codegen)
ATTR Ty [ | | appFunPP : PP_Doc  appArgPPL: {[PP_Doc]} ]
%%]

%%[(8888 codegen)
SEM Ty
  | App         loc         .   appFunPP    =   @func.appFunPP
                            .   appArgPPL   =   @arg.pp : @func.appArgPPL
  | * - App     lhs         .   appFunPP    =   @pp
                            .   appArgPPL   =   []
